{
  "name": "ashipa-mms-backend",
  "version": "2.0.0",
  "description": "The Backend providing REST APIs for the Microgrid Management System product by Ashipa Electric, Corporation",
  "main": "dist/api.bundle.js",
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --exit -r ts-node/register 'tests/**/*.ts'",
    "start": "nodemon --trace-warnings dist/api.bundle.js",
    "start:raw": "node --trace-warnings dist/api.bundle.js",
    "dev": "webpack --config-name development",
    "build": "rimraf dist/api* && webpack --config-name production",
    "prod": "node dist/api.bundle.js"
  },
  "nodemonConfig": {
    "ignore": [
      "tests/*",
      "models/*",
      "controllers/*",
      "helpers/*",
      "routes/*",
      "*.*",
      "node_modules"
    ],
    "delay": "2500"
  },
  "engines": {
    "npm": "6.14.11",
    "node": "14.16.0"
  },
  "engineStrict": true,
  "author": "Ashipa Electric, Corporation",
  "contributors": [
    {
      "name": "Israel Oluwole Olalekan",
      "email": "olaleyeisrael@gmail.com",
      "url": "https://github.com/whizyrel"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashipa-Electric/ashipa-mms-backend.git"
  },
  "keywords": [
    "Microgrid",
    "Management",
    "Software",
    "Ashipa",
    "Electric",
    "Limited"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Ashipa-Electric/ashipa-mms-backend/issues"
  },
  "homepage": "https://github.com/Ashipa-Electric/ashipa-mms-backend#readme",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.37",
    "@types/pg": "^7.14.11",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mocha": "^8.3.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "raidmaker": "^1.3.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  }
}
